{
  "name": "ufo-monorepo",
  "version": "1.0.0",
  "description": "Testing MUFON/Norwegian UFO data in PostGIS and React with OpenLayers",
  "private": true,
  "type": "module",
  "workspaces": [
    "packages/client",
    "packages/common-types",
    "packages/config",
    "packages/logger",
    "packages/dbh",
    "packages/api-functions",
    "packages/server",
    "packages/vercel"
  ],
  "scripts": {
    "build": "npm run build -ws",
    "dev:client": "npm run dev --w packages/client",
    "start:server": "npm run dev -w packages/server",
    "dev:server": "npm run watch -w packages/server",
    "predev:preview": "npm run build -w packages/client",
    "dev:preview": "npm run preview -w packages/client",
    "start": "concurrently \"npm run dev:api\" \"npm run dev:preview",
    "docs": "dot -Tpng -o docs/images/arch-flowchart.png docs/arch-flowchart.dot",
    "vercel-build": "npm run build"
  },
  "devDependencies": {
    "@rollup/pluginutils": "^5.1.0",
    "@types/debounce": "^1.2.4",
    "@types/geojson": "^7946.0.14",
    "@types/koa": "^2.15.0",
    "@types/koa__cors": "^5.0.0",
    "@types/koa-router": "^7.4.8",
    "@types/node": "^18.11.9",
    "@types/pg": "^8.11.2",
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "nodemon": "^3.1.0",
    "prettier": "^2.8.0",
    "react-intl-universal": "^2.11.3",
    "rimraf": "^6.0.1",
    "tsx": "^4.7.1",
    "typescript": "^5.2.2",
    "unplugin-parcel-macros": "^0.0.3",
    "vite": "^5.1.4",
    "vite-plugin-css-modules": "^0.0.1"
  },
  "dependencies": {
    "@koa/cors": "^5.0.0",
    "@reduxjs/toolkit": "^2.2.1",
    "@swc/cli": "^0.3.10",
    "@swc/core": "^1.4.6",
    "@vercel/node": "^3.2.15",
    "ag-grid-community": "^32.1.0",
    "ag-grid-react": "^32.1.0",
    "concurrently": "^8.2.2",
    "debounce": "^2.0.0",
    "dotenv": "^16.4.5",
    "koa": "^2.15.0",
    "koa-router": "^12.0.1",
    "ol": "^9.0.0",
    "pg": "^8.11.3",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.22.3",
    "redux": "^5.0.1",
    "vite-plugin-pwa": "^0.20.5",
    "winston": "^3.14.2"
  }
}
