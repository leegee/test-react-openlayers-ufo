{
  "version": 2,

  "installCommand": "npm install",
  "buildCommand": "npx turbo run build",
  "outputDirectory": "packages/client/build",

  "rewrites": [
    {
      "source": "/api/search",
      "destination": "/packages/vercel/buildi/api/search.js"
    },
    {
      "source": "/api/hello",
      "destination": "/api/build/hello.js"
    },
    {
      "source": "/api/:path*",
      "destination": "/packages/vercel/build/api/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/packages/client/build/$1"
    }
  ]
}
